<%- include('partials/header') %>
<link rel="stylesheet" href="/stylesheets/Login.css">
<main>
    <script src="/javascripts/validacoeslogin.js" defer></script>
    <section class="pageLogin">
      
        <h5 class="login2">LOGIN</h5><box-icon type='solid' name='cog'></box-icon><i class='bx bxs-cog'></i>
        <form class="form-auth card" method="get" action="/admin.ejs">
            <li class="email">E-mail</li>
            <input type="email" name="emailIntranet" placeholder="Digite seu email">
            <li class="password">Senha</li>
            <input id="password" type="password" name="passwordIntranet" placeholder="Digite sua senha">
            <button class="entrar" type="submit"><a href="/admin.ejs">
                    ENTRAR</a> </button>
            
        </form>
        <div class="redes">
            <h5 class="login3">Login com redes sociais</h5>
            <a href=""><img id="google" src="/images/google.png" alt="">continue com google</a>
            <a href=""><img id="facebook" src="/images/facebook.png" alt="">continue com facebook</a>
    </section>
</main>
<%- include('partials/footer') %>